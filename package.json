{
  "name": "file-templates",
  "displayName": "Fast File Creator",
  "description": "Quick create files with templates",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "icon": "icon.png",
  "publisher": "Slowmoney",
  "license": "MIT",
  "repository": "https://github.com/Slowmoney/file-templates",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-templates.show",
        "category": "Create",
        "title": "Fast File Create"
      }
    ],
    "menus": {
      "file-creator": [],
      "explorer/context": [
        {
          "command": "file-templates.show",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "submenus": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.1",
    "npm-run-all": "^4.1.5",
    "esbuild-copy-files": "^0.2.14",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "change-case": "^5.4.4"
  }
}
